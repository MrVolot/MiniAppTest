{
    "version": 2,
    "builds": [
        {
            "src": "src/app/page.tsx",
            "use": "@vercel/node",
            "config": { "includeFiles": ["src/app/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app/page.tsx"
        }
    ]
}